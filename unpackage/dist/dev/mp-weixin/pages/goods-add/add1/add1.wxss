page{
	background-color: white;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.list{
	width:750rpx;
	min-height:10rpx;
	padding-top:50rpx;
	position: absolute;
}
.list-input{
	background-color: white;
	min-height: 30rpx;
	padding:0 60rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack:justify;
	-webkit-justify-content:space-between;
	        justify-content:space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.input{
	border-bottom:1rpx solid #dddddd;
	border-radius: 5rpx;
	height:80rpx;
	width:630rpx;
}
.short-input,.long-input,.long-input2{
	border-bottom:1rpx solid #dddddd;
	border-radius: 5rpx;
	height:80rpx;
	width:120rpx;
	line-height: 80rpx;
}
.long-input{
	width:500rpx;
}
.long-input2{
	width:350rpx;
}
.button1,.button2{
	width:300rpx;
	height:80rpx;
	line-height: 80rpx;
	text-align: center;
	border:1rpx solid #dddddd;
	color:#999999;
	font-size: 30rpx;
	border-radius: 15rpx;
	margin-top:20rpx;
}
.button2{
	width:630rpx;
}
.up{
	height:90rpx;
	line-height: 90rpx;
	background-color: #B59F78;
	color:white; 
	margin-bottom:100rpx;
}
.title{
	height:80rpx;
	line-height: 80rpx;
	color:#333333;
	width:690rpx;
	margin-left:60rpx;
}
.picture{
	width:650rpx;
	margin-left:60rpx;
	padding:0rpx 20rpx 30rpx 0rpx !important;
	min-height: 100rpx !important;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
	/*justify-content: space-between;*/
}
.picture-add{
	width:192rpx;
	height:192rpx;
	border:1rpx dashed #cccccc;
	display: block;
	margin-top:20rpx;
	position: relative;
	padding: 0 !important;
}
.picture-add:before{
	position: absolute;
	top:96rpx;
	left:0;
	margin-left:46rpx;
	width:100rpx;
	content: "";
	height:2rpx;
	background-color: #CCCCCC;
}
.picture-add:after{
	position: absolute;
	top:0;
	left:96rpx;
	margin-top:46rpx;
	height:100rpx;
	content: "";
	width:1rpx;
	background-color: #CCCCCC;
}
.imagefile{
	width:192rpx;
	margin-top:20rpx;
	padding: 0 !important;
	border:none;
	height:192rpx;
	margin-right: 20rpx;
	position: relative;
}
.image{
	max-width:192rpx;
	height:192rpx;
	margin: 0 !important;
	margin-top:20rpx;
	border:1rpx solid #cccccc;
}
.remove{
	position: absolute;
	width:35rpx;
	height:35rpx;
	background-color:rgba(0,0,0,0.5);
	right:0;
	top:0;
}
.remove:before,.remove:after{
	position: absolute;
	height:20rpx;
	width:1rpx;
	background-color: #EFEFF4;
	left:18rpx;
	top:7rpx;
	content: "";
	-webkit-transform:rotate(45deg);
	        transform:rotate(45deg);
}
.remove:after{
	-webkit-transform:rotate(-45deg);
	        transform:rotate(-45deg);
}

