<view><view class="list-input"><view class="long-input2">商品规格</view><view class="short-input">库存</view><view class="short-input">价格</view></view><block wx:for="{{listPrice}}" wx:for-item="data" wx:for-index="index" wx:key="index"><view class="list-input"><view class="long-input2">{{data.goodsSku}}</view><input class="short-input" placeholder="库存" type="digit" data-event-opts="{{[['input',[['__set_model',['$0','stock','$event',[]],[[['listPrice','',index]]]]]]]}}" value="{{data.stock}}" bindinput="__e"/><input class="short-input" placeholder="价格" data-event-opts="{{[['input',[['__set_model',['$0','price','$event',[]],[[['listPrice','',index]]]]]]]}}" value="{{data.price}}" bindinput="__e"/></view></block><view class="list-input"><view data-event-opts="{{[['tap',[['navigate']]]]}}" class="button2 up" bindtap="__e">下一步</view></view></view>